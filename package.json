{
    "name":                 "amalgamate",
    "version":              "0.1.16",
    "description":

      "Merge staging Strata b-trees containing MVCC versioned records into a primary Strata b-tree. ",

    "keywords":
    [
                            "btree", "leveldb", "levelup", "binary", "mvcc",
                            "database", "json", "b-tree", "concurrent", "persistence"
    ],
    "author":               "Alan Gutierrez <alan@prettyrobots.com>",
    "contributors":         [],
    "homepage":             "http://bigeasy.github.io/amalgamate",
    "bugs":                 "https://github.com/bigeasy/amalgamate/issues",
    "license":              "MIT",
    "repository":
    {
                            "type": "git",
                            "url": "bigeasy/amalgamate"
    },
    "dependencies":
    {
                            "b-tree":                       "^2.0.0-alpha.39",
                            "designate":                    "^2.0.0-alpha.4",
                            "dilute":                       "^2.0.0-alpha.4",
                            "extant":                       "^1.0.20",
                            "homogenize":                   "^2.0.0-alpha.4",
                            "interrupt":                    "^10.1.0",
                            "rescue":                       "^7.0.0-alpha.1",
                            "riffle":                       "^2.0.0-alpha.24",
